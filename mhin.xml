<?xml version="1.0"?>

<PropertyList>

 <generic>

  <input>
   <line_separator>newline</line_separator>
   <var_separator>,</var_separator>
  <chunk>
	   <name>spdbrk</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/controls/flight/speedbrake-lever</node>
	   
   </chunk>

  <chunk>
	   <name>Flaps</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/controls/flight/flaps</node>
   </chunk>
    
   <chunk>
	   <name>gear</name>
	   <type>bool</type>
	   <node>/controls/gear/gear-down</node>
	   
   </chunk>
   
     <chunk>
	   <name>t0</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/controls/engines/engine/throttle</node>
	   
   </chunk>
   
     <chunk>
	   <name>t1</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/controls/engines/engine[1]/throttle</node>
	   
   </chunk>
   
      <chunk>
       <name>pbrake</name>
       <type>float</type>
       <format>%f</format>
    <node>/controls/gear/brake-parking</node>
   </chunk>
   
     <chunk>
       <name>zoom</name>
       <type>float</type>
       <format>%f</format>
    <node>/sim/current-view/field-of-view</node>
   </chunk>

   <chunk>
       <name>view</name>
    <node>/sim/current-view/view-number</node>
   </chunk>   
   
   
   

 </input>

 </generic>

</PropertyList>
